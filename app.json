{
  "expo": {
    "name": "TechNest",
    "slug": "ITEventsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/esciencia.png",
    "userInterfaceStyle": "light",
    "scheme": "appeventos",
    "splash": {
      "image": "./app/assets/esciencia-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/esciencia-logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.torrutech.ITEventsApp"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bab0ff22-43f9-4729-8dab-4a02500878af"
      }
    }
  }
}
